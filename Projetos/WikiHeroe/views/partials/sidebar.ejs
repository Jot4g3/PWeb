<div id="sidebar" class="col-md-2">
    <div class="d-flex flex-column flex-shrink-0 p-3 bg-orange-800 w-100">
    <a href="/characters" class="d-flex align-items-center mb-3 mb-md-0 me-md-auto link-dark text-decoration-none">
      <svg class="bi me-2" width="40" height="32"><use xlink:href="#bootstrap"/></svg>
      <span class="fs-4">Personagens</span>
    </a>
    <hr>
    <ul class="nav nav-pills flex-column mb-auto">
      <% charactersList.forEach(character => { %>
        <%- include('./btnCharacter', 
        {
            idCharacter: character.id,
            nameCharacter: character.nameCharacter
        }) %>
      <% }) %>
    </ul>
    <hr>
  </div>
</div>